status-response: "ok"
adapters:
  admaru:
    enabled: true
metrics:
  prefix: prebid
cache:
  scheme: https
  host: event-logger-dev.admaru.net
  path: /cache
  query: uuid=
auction:
  stored-requests-timeout-ms: 250
settings:
  enforce-valid-account: false
  generate-storedrequest-bidrequest-id: true
  http:
    client:
    endpoint: http://localhost:8092/stored-request
    amp-endpoint: http://localhost:8092/stored-requeest
    video-endpoint: http://localhost:8092/stored-requeest
    category-endpoint: http://localhost:8092/stored-request
  in-memory-cache:
    cache-size: 10000
    ttl-seconds: 360
    http-update:
      endpoint: http://localhost:8092/stored-request
      refresh-rate: 60000
      timeout: 2000
gdpr:
  default-value: 1
  vendorlist:
    v2:
      cache-dir: /var/tmp/vendor2
admin-endpoints:
  logging-changelevel:
    enabled: true
    path: /logging/changelevel
    on-application-port: true
    protected: false
